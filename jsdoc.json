{
    "source": {
        "include": [
            "./popup.js"
        ],
        "exclude": [
            "node_modules"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "readme": "./README.md"
    }
}